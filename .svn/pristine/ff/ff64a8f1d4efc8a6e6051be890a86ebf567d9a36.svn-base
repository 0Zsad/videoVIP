/********************* 电影Ajax遍历视频封面 ********************/
/*上半段*/
$.ajax({
	type:"post",
	url:"/videoVIP/SpbController/list.do?sp=1",
	dataType:"json",
	success:function(data){
		var zy = '';
		for(i = 0;i < 6;i++){
		  zy +=	'<div class="col-md-2">'
			 +	'<div class="thumbnail">'
			 +	'<a href="#"><img alt="....." src="'+ data[i].spfm +'" /></a>'
			 +	'<h4>剧名：'+ data[i].sppm +'</h4>'
			 +	'</div>'
			 +	'</div>'
		}
		$(".dy1").append(zy);
	}
});

