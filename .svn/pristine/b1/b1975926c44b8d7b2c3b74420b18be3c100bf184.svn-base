@charset "UTF-8";
.bg{
	background-color: #f0f0f0;
}

.head{
	width: 100%;
	height: 60px;
	position: absolute;
	margin: 0 0 10px;
	font-size: 25px;
	background-color: #2f2f2f;
	text-align: left;
	border-radius: 5px;
	color: white;
}

p{
	position: absolute;
	margin: 12px 10px;
}

table{
	position: absolute;
	margin: 70px auto;
}

tr{
	height: 40px;
	text-align: center;
	background-color: #d9d3f7;
}

th{
	height: 50px;
	background-color: #007c7c;
}